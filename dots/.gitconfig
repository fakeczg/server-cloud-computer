[safe]
	directory = /data/share/chenzigui/src/Openharmony/D2000/OH_V3.2_RE/foundation/graphic/graphic_2d
	directory = /usr/share/fonts
	directory = /data/share/chenzigui/src/Openharmony/E2000/gitlab/device_soc_phytium
	directory = /data/share/chenzigui/src/Openharmony/D2000/sync/openharmony_d2000_device
	directory = /data/share/chenzigui/src/rogue/powervr_graphics_ddk/linux/rogue
	directory = /data/share/chenzigui/src/Openharmony/D2000/PURE_OH_V4.0_RE/openharmony_d2000_device
	directory = /data/share/chenzigui/src/Openharmony/D2000/OH_V4.1_RE/openharmony_d2000_device
